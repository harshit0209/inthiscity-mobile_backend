.limiter
  .container-login100(style="background-image: url('images/bg-01.jpg');")
    .wrap-login100
      form.login100-form.validate-form(action='home' method='POST')
        span.login100-form-logo
          i.zmdi.zmdi-landscape
        span.login100-form-title.p-b-34.p-t-27 Log in
        .wrap-input100.validate-input(data-validate='Enter username')
          input.input100(type='text' name='id' placeholder='Access ID')
          span.focus-input100(data-placeholder='')
        .wrap-input100.validate-input(data-validate='Enter password')
          input.input100(type='password' name='key' placeholder='Secret Key')
          span.focus-input100(data-placeholder='')
        .container-login100-form-btn
          button.login100-form-btn Login
